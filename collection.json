{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "spec": {
      "description": "Create or update a spec for a .ts file with a class in it.",
      "factory": "./src/spec/index#spec",
      "schema": "./src/spec/schema.json"
    },
    "autospy": {
      "description": "Create an autoSpy function. Supports jest and jasmine types",
      "factory": "./src/autospy/index",
      "schema": "./src/autospy/schema.json"
    },
    "update-custom": {
      "description": "A blank schematic.",
      "factory": "./src/update-custom/index#updateCustom",
      "schema": "./src/update-custom/schema.json"
    }
  }
}
